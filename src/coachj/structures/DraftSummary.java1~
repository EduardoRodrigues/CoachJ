package coachj.structures;

/**
 * Stores information about drafting process
 *
 * @author Eduardo M. Rodrigues
 * @version 1.0
 * @date 16/08/2013
 */
public class DraftSummary {

    private short draftRound;
    private short draftPick;
    private short franchiseId;
    private String franchiseName;
    private int playerId;
    private String playerName;

    /**
     * Constructor
     */
    public DraftSummary() {
    }
    
    /**
     * Getters and setters
     */
    public short getDraftRound() {
        return draftRound;
    }

    public void setDraftRound(short draftRound) {
        this.draftRound = draftRound;
    }

    public short getDraftPick() {
        return draftPick;
    }

    public void setDraftPick(short draftPick) {
        this.draftPick = draftPick;
    }

    public String getFranchiseName() {
        return franchiseName;
    }

    public void setFranchiseName(String franchiseName) {
        this.franchiseName = franchiseName;
    }

    public String getPlayerName() {
        return playerName;
    }

    public void setPlayerName(String playerName) {
        this.playerName = playerName;
    }   

    public short getFranchiseId() {
        return franchiseId;
    }

    public void setFranchiseId(short franchiseId) {
        this.franchiseId = franchiseId;
    }

    public int getPlayerId() {
        return playerId;
    }

    public void setPlayerId(int playerId) {
        this.playerId = playerId;
    }
    
    
} // end class DraftSummary
