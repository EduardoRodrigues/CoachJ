package coachj.structures;

/**
 * Stores information about the performance of a general manager in a given
 * franchise
 *
 * @author Eduardo M. Rodrigues
 * @version 1.0
 * @date 14/08/2013
 */
public class GeneralManagerPerformance {

    /**
     * Fields
     */
    private short generalManagerId;
    private short franchiseId;
    private short yearsWithFranchise;
    private double performanceRate;

    public GeneralManagerPerformance(short generalManagerId, short franchiseId) {
        this.generalManagerId = generalManagerId;
        this.franchiseId = franchiseId;
    }
    
    /**
     * Getters and setters
     */
    public short getGeneralManagerId() {
        return generalManagerId;
    }

    public void setGeneralManagerId(short generalManagerId) {
        this.generalManagerId = generalManagerId;
    }

    public short getFranchiseId() {
        return franchiseId;
    }

    public void setFranchiseId(short franchiseId) {
        this.franchiseId = franchiseId;
    }

    public short getYearsWithFranchise() {
        return yearsWithFranchise;
    }

    public void setYearsWithFranchise(short yearsWithFranchise) {
        this.yearsWithFranchise = yearsWithFranchise;
    }

    public double getPerformanceRate() {
        return performanceRate;
    }

    public void setPerformanceRate(double performanceRate) {
        this.performanceRate = performanceRate;
    }
} // end class GeneralManagerPerformance
